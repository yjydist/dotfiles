{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "source": "$HOME/.config/fastfetch/avator.png",
        "type": "kitty-direct",
        "width": 30,
        "height": 10,
        "padding": {
            "top": 3,
            "left": 8,
            "right": 8
        }
    },
    "display": {
        "separator": " -> "
    },
    "modules": [
