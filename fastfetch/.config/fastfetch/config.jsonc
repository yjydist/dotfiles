            "key": " ",
            "keyColor": "blue",
        },
        {
            "type": "packages",
            "key": " ",
            "keyColor": "blue",  
        },
        {
            "type": "shell",
            "key": " ",
            "keyColor": "blue", 
        },
        {
            "type": "de",
            "key": " ",
            "keyColor": "blue", 
        },
        {
            "type": "uptime",
            "key": " ",
            "keyColor": "blue", 
        },
        "break",
        "colors",
        "break",
        "break"
    ]
